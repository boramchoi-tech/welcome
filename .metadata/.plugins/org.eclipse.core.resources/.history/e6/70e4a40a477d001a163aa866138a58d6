package com.chap02;

import java.util.Random;

//난수 사용해 배열의 요솟값 설정하기
public class Array02 {
	
	public int RandomArray(int[] a) {
		int max = a[0];
		for (int i = 1; i < a.length ; i++) {
			if (a[i] > max) {
				max = a[i];
			}
		}
		return max;
	}
	
	public static void main(String[] args) {
		Random rand = new Random();
		
		
	}
	

}
